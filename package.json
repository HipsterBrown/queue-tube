  "license": "MIT",
  "devDependencies": {
    "browserify": "^16.1.0",
    "standard": "^10.0.3",
    "tap": "^11.0.1",
    "watchify": "^3.10.0"
  },
  "dependencies": {
    "choo": "^6.8.0"
  }
}
